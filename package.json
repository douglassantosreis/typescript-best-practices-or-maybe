{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API desenvolvida ultilizando TS, NODE, ES, Sequelize e Postgres",
  "main": "server/server.ts",
  "scripts": {
    "start": "./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "set NODE_ENV=development&& node ./build/server.js",
    "unit-test": "set NODE_ENV=test && mocha --opts build/tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "set NODE_ENV=test && mocha --opts build/tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc"
  },
  "keywords": [
    "API",
    "typescript",
    "Node",
    "ES5"
  ],
  "author": "Douglas Santos dos Reis",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http": "0.0.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.0",
    "sequelize": "^5.11.0",
    "sequelize-cli": "^5.5.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.36",
    "@types/sequelize": "^4.28.4",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "supertest": "^4.0.2",
    "testdouble": "^3.12.2",
    "ts-node": "^8.3.0"
  }
}
